{
  "manifest_version": 3,
  "name": "connpass advanced stats",
  "description": "connpass advanced stats",
  "version": "1.0",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts": [
    {
      "matches": [
        "https://connpass.com/event/*/stats/"
      ],
      "js": [
        "./scripts/index.js"
      ]
    }
  ]
}
